Host bitbucket.org:eric_brousseau
  UseKeychain yes
  AddKeysToAgent yes
  Hostname bitbucket.org
  User git
  IdentityFile ~/.ssh/bb_rsa

Host github.com:emb417
  UseKeychain yes
  AddKeysToAgent yes
  Hostname github.com
  User git
  IdentityFile ~/.ssh/github_rsa

Host heroku.com
  UseKeychain yes
  AddKeysToAgent yes
  HostName heroku.com
  IdentityFile ~/.ssh/heroku_rsa
  IdentitiesOnly yes

Host *
  UseKeychain yes
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_rsa
